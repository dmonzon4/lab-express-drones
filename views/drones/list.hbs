<div>


<h2>Available drones</h2>

{{#each this.allDrones}}

<p>Nombre: {{this.name}}</p>
<p>Helices:{{this.propellers}}</p>
<p>Velocidad max.:{{this.maxSpeed}}</p>
<a href="/drones/{{this._id}}/edit">Editar</a>
<form action="/drones/{{this.droneToEdit._id}}/delete" method="post">
<button>Borrar Drone</button>
</form>

{{/each}}

</div>